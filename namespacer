#include <iostream>
#include <string>
using namespace std;
void ashName(string name) {
    for (int i = 0; i < name.length(); i++) {
        cout << name[i];                
        if (i < name.length() - 1) {    
            cout << " ";
        }
    }
    cout << endl; 
}
int main() {
    string name;
    cout << "Enter your name : ";
    cin >> name;
    ashName(name);  
    return 0;
}
